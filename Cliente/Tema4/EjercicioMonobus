		<script>
			// // Desarrolle un programa en el que se instancien varios objetos Producto
			// llamando a la función constructora implementada en el ejercicio anterior.
			// Seguidamente modificar el stock de los productos creados, así como su nombre y precio.
			// Implemente un documento web donde se muestre claramente el funcionamiento de todos los métodos
			//  disponibles en los objetos Producto creados.

			class Productos {
				constructor(nombre, unidades, precio) {
					this.nombre = nombre;
					this.precio = precio;
					this.unidades = unidaddes;
				}

				valorEnStock() {
					let importeTotal = this.precio * this.unidaddes;

					return importeTotal;
				}

				incrementarStock(num) {
					let incremento = incremento + this.num;

					return incremento;
				}

				disminuirStock(num) {
					let disminuir = disminuir - this.num;

					return disminuir;
				}

				get nombre() {
					return this.nombre;
				}

				set nombre(nombre) {
					this.nombre = nombre;
				}
				get unidades() {
					return this.unidades;
				}

				set unidades(unidades) {
					this.unidades = unidades;
				}
				get precio() {
					return this.precio;
				}

				set precio(precio) {
					this.precio = precio;
				}
			}

			Prodcuto1 = new Productos("Leche", 23, 2.7);
			Prodcuto2 = new Productos("Galletas", 6, 2);
			Prodcuto3 = new Productos("Pizza", 7, 5);
		</script>
